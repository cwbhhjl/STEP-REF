{
	"name": "step-ref",
	"displayName": "step-ref",
	"description": "step ifc",
	"version": "0.0.1",
	"publisher": "vscode-samples",
	"repository": "https://github.com/Microsoft/vscode-extension-samples/helloworld-sample",
	"engines": {
		"vscode": "^1.25.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:step"
	],
	"main": "./out/extension.js",
	"contributes": {
		"languages": [{
			"id":"step",
			"extensions": [
				".ifc",
				".stp",
				".p21",
				".step"
			],
			"aliases": ["STEP", "p21"]
		}]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install"
	},
	"devDependencies": {
		"@types/node": "^8.10.25",
		"tslint": "^5.11.0",
		"typescript": "^2.6.1",
		"vscode": "^1.1.22"
	}
}
